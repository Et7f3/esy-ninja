{
  "name": "esy-ninja",
  "version": "0.1.0",
  "description": "Ninja packaged for esy",
  "license": "MIT",
  "esy": {
    "buildsInSource": true,
    "exportedEnv": {
      "PATH": {
        "scope": "global",
        "val": "#{ self.bin : $PATH }"
      }
    },
    "build": [
      "python3 ./configure.py --bootstrap"
    ],
    "install": [
      "cp ./ninja #{self.bin}"
    ]
  },
  "dependencies": {
    "esy-python": "esy-packages/esy-python#4229568c55ab77a315963b255e44cf0f43223c96"
  }
}
